includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon

parameters:

    paths:
        - app/

    # Level 10 is the highest level
    level: 5

    ignoreErrors:
        -
            message: '#Call to an undefined method [a-zA-Z0-9\\_]+::get[a-zA-Z]+\(\)#'
            identifier: method.notFound
            path: app/Http/Resources/ArtworkResource.php

